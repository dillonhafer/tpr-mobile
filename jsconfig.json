{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "actions/*": ["actions/*"],
      "api/*": ["api/*"],
      "assets/*": ["assets/*"],
      "images/*": ["assets/images/*"],
      "components/*": ["components/*"],
      "constants/*": ["constants/*"],
      "fonts/*": ["fonts/*"],
      "navigation/*": ["navigation/*"],
      "notify/*": ["notify/*"],
      "reducers/*": ["reducers/*"],
      "redux-constants/*": ["redux-constants/*"],
      "screens/*": ["screens/*"],
      "utils/*": ["utils/*"]
    }
  },
  "exclude": ["../node_modules", "node_modules"]
}
