{
  "expo": {
    "name": "tpr",
    "description":
      "A fast, simple, and lightweight feed aggregator and reader.",
    "slug": "tpr",
    "privacy": "unlisted",
    "sdkVersion": "28.0.0",
    "platforms": ["ios", "android"],
    "version": "1.4.0",
    "githubUrl": "https://github.com/dillonhafer/tpr-mobile",
    "orientation": "portrait",
    "updates": {
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "icon": "./app-store-assets/logo.png",
    "splash": {
      "image": "./app-store-assets/tpr-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#7ab0b2"
    },
    "ios": {
      "buildNumber": "5",
      "usesIcloudStorage": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.dillonhafer.tpr",
      "appStoreUrl":
        "https://itunes.apple.com/us/app/the-pithy-reader/id1335273352?mt=8",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.dillonhafer.tpr",
      "versionCode": 5,
      "playStoreUrl":
        "https://play.google.com/store/apps/details?id=com.dillonhafer.tpr"
    },
    "assetBundlePatterns": ["./assets/images/**", "./assets/fonts/**"]
  }
}
